{
  "name": "frontend-admin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "openapi:clean": "rimraf src/services/api && mkdir -p src/services/api && echo \"Run pnpm openapi:gen\" > src/services/api/README.txt",
    "openapi:gen": "openapi-generator-cli generate -i http://localhost:8080/v3/api-docs -g typescript-axios -o src/services/api --skip-validate-spec --additional-properties=withSeparateModelsAndApi=true,modelPropertyNaming=original,supportsES6=true,withoutRuntimeChecks=true"
  },
  "dependencies": {
    "antd": "^5.20.2",
    "@tanstack/react-query": "^5.59.0",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "rimraf": "^5.0.10",
    "typescript": "^5.6.2",
    "vite": "^5.4.10",
    "@vitejs/plugin-react": "^4.3.1"
  }
}